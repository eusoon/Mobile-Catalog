 * @version $Revision: 2.3 $
 */
public class IFXLong extends IFXObject implements IBaseType {

	@Override
	public final native  void setString(String s) /*-{
		this.string = string;
	}-*/;

	@Override
	public  final native String getString() /*-{
    	return $wnd.cleanString(this.string);
	}-*/;
}
