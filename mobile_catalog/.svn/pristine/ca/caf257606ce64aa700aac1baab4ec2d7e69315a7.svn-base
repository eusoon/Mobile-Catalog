import my.com.infopro.mobile.client.ifx.element.ArrivalTime;
import my.com.infopro.mobile.client.ifx.element.BillingAdjustmentInd;
import my.com.infopro.mobile.client.ifx.element.CSPhoneNum;
import my.com.infopro.mobile.client.ifx.element.Carrier;
import my.com.infopro.mobile.client.ifx.element.DepartureDt;
import my.com.infopro.mobile.client.ifx.element.DepartureTime;
import my.com.infopro.mobile.client.ifx.element.FlightNumber;
import my.com.infopro.mobile.client.ifx.element.FolioNumber;
import my.com.infopro.mobile.client.ifx.element.GuestNumber;
import my.com.infopro.mobile.client.ifx.element.LodgingCharge;
import my.com.infopro.mobile.client.ifx.element.Name;
